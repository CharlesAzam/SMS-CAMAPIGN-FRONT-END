{
  "name": "azam-cms",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 4200",
    "build": "ng build --host 0.0.0.0 --port 4000",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy:prod": "ng build --aot --prod --extract-css false && aws s3 sync dist s3://azamtvmax-cms/ --delete --profile azamtvmax --acl public-read",
    "deploy:dev": "ng build --configuration=staging --extract-css false && aws s3 sync dist s3://azamtvmax-cms-dev/ --delete --profile azamtvmax --acl public-read"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.2.4",
    "@angular/cdk": "~10.2.4",
    "@angular/common": "^10.2.4",
    "@angular/compiler": "^10.2.4",
    "@angular/core": "^10.2.4",
    "@angular/forms": "^10.2.4",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^10.2.4",
    "@angular/platform-browser-dynamic": "^10.2.4",
    "@angular/router": "^10.2.4",
    "bootstrap": "^4.6.0",
    "file-saver": "^2.0.2",
    "hammerjs": "^2.0.8",
    "moment": "^2.26.0",
    "ngx-mat-select-search": "^2.2.0",
    "rxjs": "~6.4.0",
    "saturn-datepicker": "^8.0.5",
    "tslib": "^1.13.0",
    "xlsx": "^0.15.6",
    "zone.js": "~0.9.1"
  },
